{
  "name": "gam",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run build:server && npm run build:client && npm run service",
    "build": "npm run build:server && npm run build:client",
    "build:client": "cross-env RUN_ENV=client vue-cli-service build --no-clean",
    "build:server": "cross-env RUN_ENV=server vue-cli-service build --no-clean --mode server",
    "service": "cross-env RUN_ENV=server NODE_ENV=development node server\\index.js",
    "serve": "cross-env RUN_ENV=client NODE_ENV=development vue-cli-service serve",
    "service:prod": "cross-env RUN_ENV=server NODE_ENV=production node server\\index.js",
    "serve:prod": "cross-env RUN_ENV=client NODE_ENV=production vue-cli-service serve",
    "dev": "cross-env RUN_ENV=server NODE_ENV=development concurrently -r \"npm run serve \" \"npm run service\"",
    "prod": "concurrently -r \"npm run serve:prod \" \"npm run service:prod\""
  },
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.3",
    "element-ui": "^2.15.13",
    "koa": "^2.14.1",
    "koa-body": "^6.0.1",
    "koa-router": "^12.0.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "log4js": "^6.9.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "concurrently": "^7.6.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "koa-webpack": "^6.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "vue-server-renderer": "2.7.10",
    "vue-template-compiler": "^2.6.11",
    "webpack-node-externals": "^3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
